<div class="content">
  <h2>{{name}}</h2>
  <p>{{description}}</p>
  {{#if notes}}
  <h3>Important Notes</h3>
  <p>{{notes}}</p>
  {{/if}}

  <div class="row">
    <div class="col-md-12">
      <h3> Moves </h3>
      {{#each move in moves}}
      <div class="move">{{#link-to "dances.type.dance.move" move.id}} {{move.name}} {{/link-to}} </div>
      {{/each}}
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      {{outlet}}
    </div>
  </div>
</div>
